{
  "name": "tardigrades/sexy-field",
  "description": "SexyField helps you build application structure and accompanying forms.",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Dion Snoeijen",
      "email": "hallo@dionsnoeijen.nl"
    }
  ],
  "require": {
    "php": "^7.2",
    "doctrine/orm": "^2.8",
    "symfony/console": "^5",
    "symfony/form": "^5",
    "symfony/validator": "^5",
    "symfony/dependency-injection": "^5",
    "symfony/inflector": "^5",
    "symfony/yaml": "^5",
    "symfony/config": "^5",
    "symfony/event-dispatcher": "^5",
    "beberlei/assert": "^3",
    "twig/twig": "^2.12|^3.0",
    "twig/extra-bundle": "^2.12|^3.0",
    "php-coveralls/php-coveralls": "^2.0",
    "symfony/cache": "^5"
  },
  "autoload": {
    "psr-4": {
      "Tardigrades\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tardigrades\\": "test/unit/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "mockery/mockery": "^1.0",
    "mikey179/vfsstream": "^1.6"
  }
}
